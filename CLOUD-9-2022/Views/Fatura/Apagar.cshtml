@model FaturaModel
@using CLOUD_LIB 

@{
    ViewData["Title"] = "Delete";
}

<h1>Delete</h1>

<h3>Are you sure you want to delete this cart?</h3>
<div>

    <form asp-action="DeleteConfirmed">
        <input type="hidden" value="@Model.FaturaID" name="key" />
        @if (TempData["Erro"] == null)
        {<input type="submit" value="Yes" class="btn btn-danger" />}
        <a asp-action="Index" asp-controller="Home">No</a>
    </form>
</div>
